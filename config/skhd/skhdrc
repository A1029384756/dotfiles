ctrl + shift - x : yabai -m window --focus recent 
ctrl + shift - h : yabai -m window --focus west
ctrl + shift - j : yabai -m window --focus south
ctrl + shift - k : yabai -m window --focus north
ctrl + shift - l : yabai -m window --focus east
ctrl + shift - z : yabai -m window --focus stack.prev
ctrl + shift - c : yabai -m window --focus stack.next

ctrl + shift - y : yabai -m window --swap west
ctrl + shift - u : yabai -m window --swap south
ctrl + shift - i : yabai -m window --swap north
ctrl + shift - o : yabai -m window --swap east

shift + cmd - h : yabai -m window --warp west
shift + cmd - j : yabai -m window --warp south
shift + cmd - k : yabai -m window --warp north
shift + cmd - l : yabai -m window --warp east

shift + alt - r : yabai -m space --balance
alt - t : yabai -m window --toggle float --grid 4:4:1:1:2:2
